body,#container,#personal,ul{
	margin:0;
	padding:0;
	}
#container{
	margin:20px;
	background:#EEEEEE;
	border:1px solid #999999;
}
#teamname{
	margin:20px;
	width:200px;
	height:120px;
	background:#FFFFFF;
	border:1px solid #999999;
	float:left;
}
#teamname p{
	margin:20px;
	float:right;
}
#logo{
	margin:20px;
	width:80px;
	height:80px;
	background:#EEEEEE;
	border:1px solid #999999;
	}
#introduce{
	background:#EEEEEE;
	border:1px solid #999999;
	margin:20px 160px 20px 240px;
	}
#personal{
	margin:20px;
	border:1px solid;
	background:#FFFFFF;
	float:right;
	}
.clear{
	clear:both;
}
li{
	margin:20px;
	border:solid 1px;
	text-align:center;
	width:80px;
	height:80px;
	list-style-type:none;
	background:#EEEEEE;
	}
